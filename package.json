{
  "name": "docdog-lib",
  "main": "./dist/iife/docdog-lib.iife.js",
  "module": "./dist/es/docdog-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/es/docdog-lib.es.js",
      "require": "./dist/iife/docdog-lib.iife.js"
    }
  },
  "version": "0.1.0",
  "scripts": {
    "dev": "vite -c vite.iife.config.js",
    "build:iife": "vite build -c vite.iife.config.js",
    "build:es": "vite build -c vite.es.config.js",
    "build": "vite build -c vite.iife.config.js && vite build -c vite.es.config.js",
    "serve": "vite -c vite.iife.config.js preview"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "qs": "^6.10.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.10.0",
    "prettier": "2.4.1",
    "sass": "^1.49.7",
    "vfonts": "^0.1.0",
    "vite": "^2.6.4"
  }
}
